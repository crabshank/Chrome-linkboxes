{
  "manifest_version": 3,
  "name": "Linkboxes",
  "version": "2024.3",
  "options_page":  "options.html",
  "content_scripts": [
    {
	      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
	"tabs",
	"activeTab",
	"history",
	"storage"
  ],
  "action": {
    "default_icon": "linkbox.png",
    "default_popup": "index.html"
  },
  	"icons": {
        "128": "linkbox.png"
    }
}
